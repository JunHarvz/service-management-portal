generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Account {
  id           String   @id
  username     String   @unique
  password     String
  email        String
  accessToken  String
  createdAt    DateTime
  updatedAt    DateTime
  deleteAt     DateTime
  lastLoggedIn DateTime
  userId       String
}
